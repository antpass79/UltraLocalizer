  <!--TS PRESET FACTORY NAME - START-->
  <LocalizationSection InternalNamespace="FactoryGroups">

    <Concept Id="FACTORY">
      <Comments>Factory name is valid for each application in 3d Preset Menu</Comments>
      <String Context="Generic_Medium">FACTORY</String>
    </Concept>

    <Concept Id="GENERAL">
      <Comments>This name is valid for each application in 3d Preset Menu</Comments>
      <String Context="Generic_Medium">GENERAL</String>
    </Concept>

    <Concept Id="SURFACE">
      <Comments>This name is valid for each application (except OB-Fetal) in 3d Preset Menu</Comments>
      <String Context="Generic_Medium">SURFACE</String>
    </Concept>

    <Concept Id="BABY FACE">
      <Comments>This name is valid only for OB-Fetal application in 3d Preset Menu</Comments>
      <String Context="Generic_Medium">BABY FACE</String>
    </Concept>

    <Concept Id="SPINE">
      <Comments>This name is valid only for OB-Fetal application in 3d Preset Menu</Comments>
      <String Context="Generic_Medium">SPINE</String>
    </Concept>

    <Concept Id="DEEP">
      <Comments>This name is valid only for Gynecological application in 3d Preset Menu</Comments>
      <String Context="Generic_Medium">DEEP</String>
    </Concept>

  </LocalizationSection>
