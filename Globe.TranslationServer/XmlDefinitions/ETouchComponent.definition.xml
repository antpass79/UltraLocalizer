<?xml version="1.0" encoding="utf-8" ?>
<LocalizationResource ComponentNamespace="ETouchComponent" Language="Definition" Version="1">
  <LocalizationSection InternalNamespace="Factory">
    <Concept Id="Default">
      <Comment>Name of a configuration in the E-Touch configuration editor's list</Comment>
      <String Context="Generic_Medium">Default</String>
    </Concept>
    <Concept Id="EXAMPLE">
      <Comment>Name of a configuration in the E-Touch configuration editor's list</Comment>
      <String Context="Generic_Medium">Example</String>
    </Concept>
  </LocalizationSection>
  <LocalizationSection InternalNamespace="EDITOR">
    <Concept Id="EDITOR_NAME">
      <Comment>Name of the e-touch configuration editor in the preset menu list</Comment>
      <String Context="Generic_Medium">E-Touch</String>
    </Concept>
    <Concept Id="RECORDHEADER_LBL">
      <String Context="Generic_Medium">Record</String>
    </Concept>
    <Concept Id="RECORD_BTN">
      <String Context="Generic_Medium">Recording Procedure</String>
    </Concept>
    <Concept Id="BUTTONSEDITORHEADER_LBL">
      <String Context="Generic_Medium">Buttons Editor</String>
    </Concept>
    <Concept Id="MOVE_BTN">
      <String Context="Generic_Medium">Move selected button</String>
    </Concept>
    <Concept Id="DELETE_BTN">
      <String Context="Generic_Medium">Delete selected button</String>
    </Concept>
    <Concept Id="TABOPERATIONSHEADER_LBL">
      <String Context="Generic_Medium">Tab Editor</String>
    </Concept>
    <Concept Id="NEWTAB_BTN">
      <String Context="Generic_Medium">New Tab</String>
    </Concept>
    <Concept Id="DELETETAB_BTN">
      <String Context="Generic_Medium">Delete selected Tab</String>
    </Concept>
    <Concept Id="MOVETABLEFT_BTN">
      <String Context="Generic_Medium">Move Left</String>
    </Concept>
    <Concept Id="MOVETABRIGHT_BTN">
      <String Context="Generic_Medium">Move Right</String>
    </Concept>
    <Concept Id="EMPTY_COMMAND_MSG">
      <String Context="Generic_Warning">Command list is empty, macro will not be saved</String>
    </Concept>
    <Concept Id="NO_FREE_BUTTON_MSG">
      <String Context="Generic_Warning">To add a new sequence, the selected tab must have an empty button. Select another tab or move/delete an existing sequence before recording a new one.</String>
    </Concept>
    <Concept Id="EMPTYLABEL_WARNING">
      <Comment>Error when trying to set an empty label on a button</Comment>
      <String Context="Generic_Warning">Cannot have empty label</String>
    </Concept>
    <Concept Id="TABLABELTOOLONG_WARNING">
      <Comment>Error when trying to set a tab label too long to fit in the touchscreen</Comment>
      <String Context="Generic_Warning">Tab label too long (must be 15 characters or less)</String>
    </Concept>
    <Concept Id="DUPLICATE_TAB_LABEL_WARNING">
      <Comment>Error when trying to save a configuration where 2 or more tabs have the same name. List o duplicate labels follows the ":"</Comment>
      <String Context="Generic_Warning">Duplicate tab label(s) found:</String>
    </Concept>
    <Concept Id="DUPLICATE_BUTTON_LABEL_WARNING">
      <Comment>Error when trying to save a configuration where 2 or more buttons have the same name. List o duplicate labels follows the ":"</Comment>
      <String Context="Generic_Warning">Duplicate button label(s) found:</String>
    </Concept>
    <Concept Id="ETOUCH_TABCOMMAND">
      <Comment>Default name of a new tab</Comment>
      <String Context="TS_TabName">eTouch</String>
    </Concept>
    <Concept Id="RESERVED_BUTTON">
      <String Context="TS_AButton">Reserved</String>
    </Concept>
    <Concept Id="MOVE_TOOLTIP">
      <String Context="Generic_Long">Select destination button</String>
    </Concept>
    <Concept Id="LOAD_CONFSET_FAILED">
      <Comment>Warning message when a corrupted file is found</Comment>
      <String Context="Generic_Long">Loading failed - corrupted configuration</String>
    </Concept>
    <Concept Id="LOAD_CONFSET_FAILED_LOAD_FACTORY">
      <Comment>Warning message when a corrupted file is found and the corresponding factory is loaded</Comment>
      <String Context="Generic_Warning">Loading failed - corrupted configuration. The corresponding Factory has been loaded.</String>
    </Concept>
    <Concept Id="MODIFICATIONS_ACTIVE_MESSAGE">
      <Comment>Message shown on saving a configuration</Comment>
      <String Context="Generic_Warning">Modifications will be available after starting a new exam</String>
    </Concept>
  </LocalizationSection>
  <LocalizationSection InternalNamespace="RECORDINGSTATE_MESSAGES">
    <Concept Id="ISRECORDING_MSG">
      <String Context="MD_IconArea">Press E-Touch to stop recording</String>
    </Concept>
    <Concept Id="READYTORECORD_MSG">
      <String Context="MD_IconArea">Press E-Touch to start recording</String>
    </Concept>
  </LocalizationSection>
</LocalizationResource>

